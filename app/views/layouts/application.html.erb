	<%#= javascript_include_tag "jquery.preloader.js" %>
	<%= javascript_include_tag "touchTouch.jquery.js" %>

	<script type="text/javascript">
		if($(window).width()>1024){
			document.write("<"+"script src='/assets/jquery.preloader.js'></"+"script>");
		}
		// if($(window).width()>1024){document.write("<"+"script src='js/jquery.preloader.js'></"+"script>");}
	</script>

	<script>		
		jQuery(window).load(function() {	
			$x = $(window).width();		
			if($x > 1024){			
				jQuery("#content .row").preloader();    
			}	
	
			jQuery('.magnifier').touchTouch();			
    		jQuery('.spinner').animate({'opacity':0},
    			1000,
    			'easeOutCubic',
    			function (){jQuery(this).css('display','none')}
    		);	
  		}); 
	</script>
</head>
